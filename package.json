{
  "name": "solar-moon-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aws-amplify/ui-react": "^6.12.0",
    "@hookform/resolvers": "^5.2.2",
    "@stripe/react-stripe-js": "^5.4.1",
    "@stripe/stripe-js": "^8.5.3",
    "@szhsin/react-menu": "^4.5.0",
    "@tippyjs/react": "^4.2.6",
    "@wojtekmaj/react-daterange-picker": "^7.0.0",
    "aws-amplify": "^6.15.6",
    "axios": "^1.13.5",
    "chart.js": "^4.5.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.1",
    "react-chartjs-2": "^5.2.0",
    "react-csv": "^2.2.2",
    "react-data-grid": "^7.0.0-beta.59",
    "react-day-picker": "^9.11.3",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "react-intl": "^7.1.11",
    "react-router-dom": "^7.12.0",
    "react-select": "^5.10.2",
    "react-timezone-select": "^3.2.7",
    "react-toastify": "^11.0.5",
    "sass": "^1.92.1",
    "tailwindcss": "^3.4.7",
    "usehooks-ts": "^3.1.1",
    "uuid": "^13.0.0",
    "yup": "^1.7.0"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx && stylelint \"**/*.{css,scss}\"",
    "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix && stylelint \"**/*.{css,scss}\" --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@eslint/js": "^9.39.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/chart.js": "^2.9.41",
    "@types/classnames": "^2.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/testing-library__react": "^10.0.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "autoprefixer": "^10.4.23",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "baseline-browser-mapping": "^2.9.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "jsdom": "^27.4.0",
    "prettier": "^3.6.2",
    "stylelint": "^16.24.0",
    "stylelint-prettier": "^5.0.2",
    "type-coverage": "^2.29.7",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.17"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/graphql/**/*",
      "!src/aws-exports.js",
      "!src/index.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/graphql/",
      "src/aws-exports.js",
      "src/index.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(uuid|usehooks-ts))"
    ],
    "moduleNameMapper": {
      "^react-router-dom$": "<rootDir>/node_modules/react-router-dom/dist/index.js",
      "^react-router$": "<rootDir>/node_modules/react-router/dist/development/index.js",
      "^react-router/dom$": "<rootDir>/node_modules/react-router/dist/development/dom-export.js"
    }
  },
  "proxy": "https://app.solarmoonanalytics.com"
}
